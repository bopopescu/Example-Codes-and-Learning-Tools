P_006DA5D0 .param/l "JMP" 2 11, C4<0010>;
P_006DA5E4 .param/l "LD" 2 11, C4<0000>;
P_006DA5F8 .param/l "ST" 2 11, C4<0011>;
v00731DF0_0 .var "IR", 15 0;
v006D3B18 .array "R", 2 0, 15 0;
v006D3B70_0 .net "clock", 0 0, v0072ED10_0; 1 drivers
v006D33F8_0 .var/i "i", 31 0;
v006D3450 .array "m", 4095 0, 7 0;
v006D34A8_0 .var "pc0", 15 0;
E_006D9780 .event posedge, v006D3B70_0;
    .scope S_006DA508;
T_0 ;
   %ix/load 1, 0, 0;
   %ix/load 3, 1, 0;
   %set/av v006D3B18, 0, 16;
   %ix/load 1, 0, 0;
   %ix/load 3, 2, 0;
   %set/av v006D3B18, 0, 16;
    %vpi_call 2 21 "$readmemh", "mcu0m.hex", v006D3450;
    %set/v v006D33F8_0, 0, 32;
T_0.0 ;
    %load/v 8, v006D33F8_0, 32;
   %cmpi/s 8, 32, 32;
    %jmp/0xz T_0.1, 5;
    %ix/load 0, 1, 0;
    %load/vp0/s 24, v006D33F8_0, 32;
    %ix/get/s 3, 24, 32;
    %load/av 8, v006D3450, 8;
    %ix/getv/s 3, v006D33F8_0;
    %load/av 16, v006D3450, 8;
    %vpi_call 2 23 "$display", "%x: %x", v006D33F8_0, T<8,16,u>;
    %ix/load 0, 2, 0;
    %load/vp0/s 8, v006D33F8_0, 32;
    %set/v v006D33F8_0, 8, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .thread T_0;
    .scope S_006DA508;
T_1 ;
    %wait E_006D9780;
    %ix/load 3, 1, 0;
    %mov 4, 0, 1;
    %ix/load 0, 1, 0;
    %load/avp0 24, v006D3B18, 32;
    %ix/get 3, 24, 32;
    %load/av 8, v006D3450, 8;
    %ix/load 3, 1, 0;
    %mov 4, 0, 1;
    %load/av 24, v006D3B18, 16;
    %ix/get 3, 24, 16;
    %load/av 16, v006D3450, 8;
    %set/v v00731DF0_0, 8, 16;
    %ix/load 3, 1, 0;
    %mov 4, 0, 1;
    %load/av 8, v006D3B18, 16;
    %set/v v006D34A8_0, 8, 16;
    %ix/load 3, 1, 0;
    %mov 4, 0, 1;
    %load/av 8, v006D3B18, 16;
    %mov 24, 0, 16;
    %addi 8, 2, 32;
   %ix/load 1, 0, 0;
   %ix/load 3, 1, 0;
   %set/av v006D3B18, 8, 16;
    %ix/load 1, 12, 0;
    %mov 4, 0, 1;
    %jmp/1 T_1.0, 4;
    %load/x1p 8, v00731DF0_0, 4;
    %jmp T_1.1;
T_1.0 ;
    %mov 8, 2, 4;
T_1.1 ;
; Save base=8 wid=4 in lookaside.
    %cmpi/u 8, 0, 4;
    %jmp/1 T_1.2, 6;
    %cmpi/u 8, 3, 4;
    %jmp/1 T_1.3, 6;
    %cmpi/u 8, 4, 4;
    %jmp/1 T_1.4, 6;
    %cmpi/u 8, 1, 4;
    %jmp/1 T_1.5, 6;
    %cmpi/u 8, 2, 4;
    %jmp/1 T_1.6, 6;
    %cmpi/u 8, 5, 4;
    %jmp/1 T_1.7, 6;
    %jmp T_1.8;
T_1.2 ;
    %load/v 24, v00731DF0_0, 12; Only need 12 of 16 bits
    %movi 36, 0, 20;
; Save base=24 wid=32 in lookaside.
    %addi 24, 1, 32;
    %ix/get 3, 24, 32;
    %load/av 8, v006D3450, 8;
    %load/v 24, v00731DF0_0, 12; Only need 12 of 16 bits
; Save base=24 wid=12 in lookaside.
    %ix/get 3, 24, 12;
    %load/av 16, v006D3450, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 0, 0;
   %set/av v006D3B18, 8, 16;
    %jmp T_1.8;
T_1.3 ;
    %ix/load 3, 0, 0;
    %mov 4, 0, 1;
    %load/av 8, v006D3B18, 16;
    %load/v 24, v00731DF0_0, 12; Only need 12 of 16 bits
    %movi 36, 0, 20;
; Save base=24 wid=32 in lookaside.
    %addi 24, 1, 32;
    %ix/get 3, 24, 32;
   %jmp/1 t_0, 4;
   %ix/load 1, 0, 0;
   %set/av v006D3450, 8, 8;
t_0 ;
    %load/v 24, v00731DF0_0, 12; Only need 12 of 16 bits
; Save base=24 wid=12 in lookaside.
    %ix/get 3, 24, 12;
   %jmp/1 t_1, 4;
   %ix/load 1, 0, 0;
   %set/av v006D3450, 16, 8;
t_1 ;
    %jmp T_1.8;
T_1.4 ;
    %ix/load 3, 0, 0;
    %mov 4, 0, 1;
    %load/av 8, v006D3B18, 16;
    %load/v 40, v00731DF0_0, 12; Only need 12 of 16 bits
    %movi 52, 0, 20;
; Save base=40 wid=32 in lookaside.
    %addi 40, 1, 32;
    %ix/get 3, 40, 32;
    %load/av 24, v006D3450, 8;
    %load/v 40, v00731DF0_0, 12; Only need 12 of 16 bits
; Save base=40 wid=12 in lookaside.
    %ix/get 3, 40, 12;
    %load/av 32, v006D3450, 8;
    %cmp/u 8, 24, 16;
    %mov 8, 5, 1;
   %ix/load 3, 2, 0;
   %ix/load 1, 15, 0;
   %set/av v006D3B18, 8, 1;
    %ix/load 3, 0, 0;
    %mov 4, 0, 1;
    %load/av 8, v006D3B18, 16;
    %load/v 40, v00731DF0_0, 12; Only need 12 of 16 bits
    %movi 52, 0, 20;
; Save base=40 wid=32 in lookaside.
    %addi 40, 1, 32;
    %ix/get 3, 40, 32;
    %load/av 24, v006D3450, 8;
    %load/v 40, v00731DF0_0, 12; Only need 12 of 16 bits
; Save base=40 wid=12 in lookaside.
    %ix/get 3, 40, 12;
    %load/av 32, v006D3450, 8;
    %cmp/u 8, 24, 16;
    %mov 8, 4, 1;
   %ix/load 3, 2, 0;
   %ix/load 1, 14, 0;
   %set/av v006D3B18, 8, 1;
    %jmp T_1.8;
T_1.5 ;
    %ix/load 3, 0, 0;
    %mov 4, 0, 1;
    %load/av 8, v006D3B18, 16;
    %load/v 40, v00731DF0_0, 12; Only need 12 of 16 bits
    %movi 52, 0, 20;
; Save base=40 wid=32 in lookaside.
    %addi 40, 1, 32;
    %ix/get 3, 40, 32;
    %load/av 24, v006D3450, 8;
    %load/v 40, v00731DF0_0, 12; Only need 12 of 16 bits
; Save base=40 wid=12 in lookaside.
    %ix/get 3, 40, 12;
    %load/av 32, v006D3450, 8;
    %add 8, 24, 16;
   %ix/load 1, 0, 0;
   %ix/load 3, 0, 0;
   %set/av v006D3B18, 8, 16;
    %jmp T_1.8;
T_1.6 ;
    %load/v 8, v00731DF0_0, 12; Select 12 out of 16 bits
    %mov 20, 0, 4;
   %ix/load 1, 0, 0;
   %ix/load 3, 1, 0;
   %set/av v006D3B18, 8, 16;
    %jmp T_1.8;
T_1.7 ;
    %movi 8, 14, 5;
    %ix/load 3, 2, 0;
    %mov 4, 0, 1;
    %jmp/1 T_1.9, 4;
    %ix/get/s 0, 8, 5;
T_1.9 ;
    %load/avx.p 8, v006D3B18, 0;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_1.10, 8;
    %load/v 8, v00731DF0_0, 12; Select 12 out of 16 bits
    %mov 20, 0, 4;
   %ix/load 1, 0, 0;
   %ix/load 3, 1, 0;
   %set/av v006D3B18, 8, 16;
T_1.10 ;
    %jmp T_1.8;
T_1.8 ;
    %vpi_call 2 40 "$display", "%4dns PC=%x IR=%x, SW=%x, A=%d", $stime, v006D34A8_0, v00731DF0_0, &A<v006D3B18, 2>, &A<v006D3B18, 0>;
    %jmp T_1;
    .thread T_1;
    .scope S_006DA480;
T_2 ;
    %set/v v0072ED10_0, 0, 1;
    %end;
