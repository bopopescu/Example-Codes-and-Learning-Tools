S_006DA480 .scope module, "main" "main" 2 44;
 .timescale 0 0;
v0072ED10_0 .var "clock", 0 0;
S_006DA508 .scope module, "mcu0" "mcu" 2 47, 2 10, S_006DA480;
 .timescale 0 0;
P_006DA594 .param/l "ADD" 2 11, C4<0001>;
