    .scope S_006DA480;
T_3 ;
    %delay 10, 0;
    %load/v 8, v0072ED10_0, 1;
    %inv 8, 1;
    %set/v v0072ED10_0, 8, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_006DA480;
T_4 ;
    %delay 2000, 0;
    %vpi_call 2 51 "$finish";
    %end;
    .thread T_4;
