#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00643248 .scope module, "main" "main" 2 1;
 .timescale 0 0;
L_006432D0/d .functor NOT 1, v00645150_0, C4<0>, C4<0>, C4<0>;
L_006432D0 .delay (2,2,2) L_006432D0/d;
L_0064E630/d .functor NOT 1, v00645150_0, C4<0>, C4<0>, C4<0>;
L_0064E630 .delay (3,3,3) L_0064E630/d;
v00645150_0 .var "clk", 0 0;
v0064E550_0 .net "nclk1", 0 0, L_006432D0; 1 drivers
v0064E5A8_0 .net "nclk2", 0 0, L_0064E630; 1 drivers
    .scope S_00643248;
T_0 ;
    %set/v v00645150_0, 0, 1;
    %vpi_call 2 9 "$monitor", "%dns monitor: clk=%b nclk1=%d nclk2=%d", $stime, v00645150_0, v0064E550_0, v0064E5A8_0;
    %vpi_call 2 10 "$dumpfile", "delay.vcd";
    %vpi_call 2 11 "$dumpvars";
    %end;
    .thread T_0;
    .scope S_00643248;
T_1 ;
    %delay 10, 0;
    %load/v 8, v00645150_0, 1;
    %mov 9, 0, 31;
    %addi 8, 1, 32;
    %set/v v00645150_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_00643248;
T_2 ;
    %delay 100, 0;
    %vpi_call 2 18 "$finish";
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "delay.v";
